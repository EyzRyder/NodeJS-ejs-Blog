{
    "version": 2,
    "builds": [
        {
            "src": "./app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ], 
    "redirects": [
        {
            "source": "/",
            "destination": "/view/index.ejs"
        },
        {
            "source": "/blogs",
            "destination": "/view/index.ejs"
        },
        {
            "source": "/about",
            "destination": "/view/about.ejs"
        }
    ]
}